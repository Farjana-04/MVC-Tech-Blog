
{{#if posts.length}}
<section>
  <h2>Your Posts</h2>
    {{#each posts as |post|}}
      {{> dashboard-postinfo post}}
      <a href="/dashboard/update/{{post.id}}">Update post</a>
    {{/each}}

</section>
{{/if}}

<button type="submit" class="btn"><a class="link" href="/dashboard/new">Add New Post</a></button>


<script src="js/create-post.js"></script>